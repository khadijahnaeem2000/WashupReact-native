{
  "cli": {
    "version": ">= 7.6.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "android": {
        "gradleCommand": ":app:assembleDevelopmentDebug"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "production": {
      "android": {
        "gradleCommand": ":app:assembleProductionRelease"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}